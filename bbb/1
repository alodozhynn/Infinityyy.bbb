local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Infinity Big Blox üú≤", HidePremium = false, SaveConfig = true, ConfigFolder = "BigBloxHub"})

-- Aba Teleportes
local TeleportTab = Window:MakeTab({
	Name = "Teleportes",
	Icon = "rbxassetid://7072719338",
	PremiumOnly = false
})

OrionLib:MakeNotification({
	Name = "Infinityyy.bbb 1.0",
	Content = "Made by Nightscript Studios!",
	Image = "rbxassetid://8834748103",
	Time = 5
})
local Section = TeleportTab:AddSection({
	Name = "Teleportes no lobby"
})

TeleportTab:AddButton({
	Name = "Dentro da Pista",
	Callback = function()
      		local player = game.Players.LocalPlayer
      		local character = player.Character or player.CharacterAdded:Wait()
      		character:SetPrimaryPartCFrame(CFrame.new(-778, 52, 400))
      		print("Teleported to Dentro da Pista")
  	end    
})
local Section = TeleportTab:AddSection({
	Name = "Dentro das casas"
})
TeleportTab:AddButton({
	Name = "Dentro da Casa 22",
	Callback = function()
      		local player = game.Players.LocalPlayer
      		local character = player.Character or player.CharacterAdded:Wait()
      		character:SetPrimaryPartCFrame(CFrame.new(215, 30, -1573))
      		print("Teleported to Dentro da Casa 22")
  	end    
})

TeleportTab:AddButton({
	Name = "Dentro da Casa 18",
	Callback = function()
      		local player = game.Players.LocalPlayer
      		local character = player.Character or player.CharacterAdded:Wait()
      		character:SetPrimaryPartCFrame(CFrame.new(-875, 60, 324))
      		print("Teleported to Casa 18")
  	end    
})

-- Aba Provas
local proofsTab = Window:MakeTab({
	Name = "Provas",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

-- Bot√£o Prova de Matem√°tica (Calculadora)
proofsTab:AddButton({
	Name = "Prova de matem√°tica (calculadora)",
	Callback = function()
      		loadstring(game:HttpGet("https://rawscripts.net/raw/Calculator_229"))()
      		print("Calculadora de Matem√°tica carregada")
  	end    
})

-- Label de Mais Funcionalidades
proofsTab:AddLabel("Mais funcionalidades em breve!")
})
OrionLib:Init()
